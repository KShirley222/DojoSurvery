@{
    ViewData["Title"] = "Home Page";
}
@using DojoSurvey.Models
@model Survey

<div class="container">
    <div class="row">
        <h1>Submitted info:</h1>
    </div>
    <div class="row">
        <p class="col-md-6">Name:</p>
        <p class="col-md-6">@Model.Name</p>
    </div>
    <div class="row">
        <p class="col-md-6">Location:</p>
        <p class="col-md-6">@Model.Location</p>
    </div>
    <div class="row">
        <p class="col-md-6">Language:</p>
        <p class="col-md-6">@Model.Language</p>
    </div>
        <div class="row">
        <p class="col-md-6">Comments:</p>
        <p class="col-md-6">@Model.Comment</p>
    </div>
</div>